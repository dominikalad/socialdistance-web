on from \"./LikeButton\";\nimport ButtonUtil from \"../../util/ButtonUtil\";\nimport Comments from \"./Comments\";\nimport CommentForm from \"./CommentForm\";\n\nimport withStyles from \"@material-ui/core/styles/withStyles\";\nimport {\n  Dialog,\n  DialogContent,\n  CircularProgress,\n  Grid,\n  Typography,\n} from \"@material-ui/core\";\nimport CloseIcon from \"@material-ui/icons/Close\";\nimport UnfoldMoreIcon from \"@material-ui/icons/UnfoldMore\";\nimport ChatIcon from \"@material-ui/icons/Chat\";\nimport theme from \"../../util/theme\";\n\nconst styles = (theme) => ({\n  ...theme.styles,\n  profileImage: {\n    maxWidth: 200,\n    height: 200,\n    borderRadius: \"50%\",\n    objectFit: \"cover\",\n  },\n  dialogContent: {\n    padding: 20,\n  },\n  closeButton: {\n    position: \"absolute\",\n    left: \"92%\",\n  },\n  expandButton: {\n    alignSelf: \"flex-start\",\n  },\n});\n\nclass ScreamDialog extends Component {\n  state = {\n    open: false,\n    newPath: \"\",\n    oldPath: \"\",\n  };\n\n  componentDidMount() {\n    if (this.props.openDialog) {\n      this.handleOpen();\n    }\n  }\n\n  handleOpen = () => {\n    let oldPath = window.location.pathname;\n    const { userHandle, screamId } = this.props;\n    const newPath = `/users/${userHandle}/scream/${screamId}`;\n\n    //FIX : its bugging POST comment\n    if (oldPath === newPath) oldPath = `/users/${userHandle}`;\n\n    window.history.pushState(null, null, newPath);\n\n    this.setState({ open: true, newPath, oldPath });\n    this.props.getScream(this.props.screamId);\n  };\n\n  handleClose = () => {\n    window.history.pushState(null, null, this.state.oldPath);\n    this.setState({ open: false });\n    this.props.clearErrors();\n  };\n\n  render() {\n    const {\n      classes,\n      scream: {\n        screamId,\n        body,\n        createdAt,\n        likeCount,\n        commentCount,\n        userImage,\n        userHandle,\n        comments,\n      },\n      UI: { loading },\n    } = this.props;\n\n    const dialogMarkup = loading ? (\n      <CircularProgress size={200} />\n    ) : (\n      <Grid container spacing={16}>\n        <Grid item sm={5}>\n          <img src={userImage} alt=\"Profile\" className={classes.profileImage} />\n        </Grid>\n        <Grid item sm={7}>\n          <Typography\n            component={Link}\n            color=\"primary\"\n            variant=\"h5\"\n            to={`/users/${userHandle}`}\n          >\n            @{userHandle}\n          </Typography>\n          <hr className={classes.invisibleSeparator} />\n          <Typography variant=\"body2\" color=\"textSecondary\">\n            {dayjs(createdAt).format(\"h:mm a, MMMM DD YYYY\")}\n          </Typography>\n          <hr className={classes.invisibleSeparator} />\n          <Typography variant=\"body1\">{body}</Typography>\n          <LikeButton screamId={screamId} />\n          <span>{likeCount} hearts</span>\n          <ButtonUtil tip=\"comments\">\n            <ChatIcon color=\"primary\" />\n          </ButtonUtil>\n          <span>{commentCount} comments</span>\n        </Grid>\n        {/* {(comments && comments.length > 0) && <hr className={classes.visibleSeparator} />} */}\n        <CommentForm screamId={screamId} />\n        <Comments comments={comments} />\n      </Grid>\n    );\n\n    return (\n      <Fragment>\n        <ButtonUtil\n          onClick={this.handleOpen}\n          tip=\"Expand scream\"\n          tipClassName={classes.expandButton}\n        >\n          <UnfoldMoreIcon color=\"primary\" />\n        </ButtonUtil>\n        <Dialog\n          open={this.state.open}\n          onClose={this.handleClose}\n          fullWidth\n          maxWidth=\"sm\"\n        >\n          <ButtonUtil\n            tip=\"Close\"\n            onClick={this.handleClose}\n            tipClassName={classes.closeButton}\n          >\n            <CloseIcon />\n          </ButtonUtil>\n          <DialogContent className={classes.dialogContent}>\n            {dialogMarkup}\n          </DialogContent>\n        </Dialog>\n      </Fragment>\n    );\n  }\n}\n\nScreamDialog.propTypes = {\n  getScream: PropTypes.func.isRequired,\n  screamId: PropTypes.string.isRequired,\n  userHandle: PropTypes.string.isRequired,\n  scream: PropTypes.object.isRequired,\n  UI: PropTypes.object.isRequired,\n  clearErrors: PropTypes.func.isRequired,\n};\n\nconst mapStateToProps = (state) => ({\n  scream: state.data.scream,\n  UI: state.UI,\n});\n\nconst mapActionsToProps = {\n  getScream,\n  clearErrors,\n};\n\nexport default connect(\n  mapStateToProps,\n  mapActionsToProps\n)(withStyles(styles)(ScreamDialog));\n","/Users/dominika/Projects/socialdistance-web/src/components/scream/Scream.js",[],"/Users/dominika/Projects/socialdistance-web/src/components/scream/Comments.js",[],"/Users/dominika/Projects/socialdistance-web/src/components/scream/CommentForm.js",[],"/Users/dominika/Projects/socialdistance-web/src/pages/user.js",[],"/Users/dominika/Projects/socialdistance-web/src/components/profile/StaticProfile.js",[],"/Users/dominika/Projects/socialdistance-web/src/components/layout/Navbar.js",[],"/Users/dominika/Projects/socialdistance-web/src/components/layout/Notifications.js",[],"/Users/dominika/Projects/socialdistance-web/src/util/ScreamSkeleton.js",[],"/Users/dominika/Projects/socialdistance-web/src/components/profile/Profile.js",[],"/Users/dominika/Projects/socialdistance-web/src/util/ProfileSkeleton.js",[],"/Users/dominika/Projects/socialdistance-web/src/components/profile/EditDetails.js",["139"],"/Users/dominika/Projects/socialdistance-web/src/components/scream/DeleteScream.js",[],"/Users/dominika/Projects/socialdistance-web/src/components/scream/LikeButton.js",[],{"ruleId":"140","replacedBy":"141"},{"ruleId":"142","replacedBy":"143"},{"ruleId":"140","replacedBy":"144"},{"ruleId":"142","replacedBy":"145"},{"ruleId":"146","severity":1,"message":"147","line":1,"column":1,"nodeType":"148","endLine":145,"endColumn":3},{"ruleId":"146","severity":1,"mes